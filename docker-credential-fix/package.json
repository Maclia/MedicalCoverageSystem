{
  "name": "docker-credential-fix",
  "version": "1.0.0",
  "description": "Automated Docker credential helper diagnostic and repair tool",
  "main": "src/index.js",
  "bin": {
    "docker-credential-fix": "bin/docker-credential-fix"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": ["darwin", "linux", "win32"],
  "preferGlobal": true,
  "keywords": [
    "docker",
    "credentials",
    "diagnostic",
    "fix",
    "authentication"
  ],
  "author": "Docker Credential Fix Tool",
  "license": "MIT",
  "dependencies": {
    "inquirer": "^9.0.0",
    "chalk": "^5.0.0",
    "ora": "^7.0.0",
    "cli-table3": "^0.6.0"
  },
  "optionalDependencies": {
    "express": "^4.18.0",
    "ws": "^8.0.0",
    "open": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "start": "node bin/docker-credential-fix",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "build": "node scripts/build.js",
    "publish": "node scripts/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docker-credential-fix/docker-credential-fix.git"
  },
  "bugs": {
    "url": "https://github.com/docker-credential-fix/docker-credential-fix/issues"
  },
  "homepage": "https://github.com/docker-credential-fix/docker-credential-fix#readme"
}